			<AnimationGroup parentKey="FadeRank4Anim" setToFinalAlpha="true">
				<Alpha parentKey="Alpha" childKey="BackgroundFrame.Rank4RingBgGlow" smoothing="IN_OUT" duration="1.4" order="1" fromAlpha="1" toAlpha="0" />
			</AnimationGroup>
		</Animations>
	</Frame>
</Ui>