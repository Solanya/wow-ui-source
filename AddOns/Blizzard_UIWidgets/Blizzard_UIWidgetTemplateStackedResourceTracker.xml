<Ui xmlns="http://www.blizzard.com/wow/ui/" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.blizzard.com/wow/ui/
..\..\FrameXML\UI.xsd">
	<Frame name="UIWidgetTemplateStackedResourceTracker_ResourceFrame" inherits="UIWidgetTemplateTooltipFrame" virtual="true">
		<Layers>
			<Layer level="OVERLAY">
				<Texture parentKey="Icon">
					<Anchors>
						<Anchor point="LEFT" />
					</Anchors>
				</Texture>
				<FontString parentKey="Text" inherits="GameFontNormal" justifyH="CENTER">
					<Size x="67"/>
					<Anchors>
						<Anchor point="LEFT" relativeKey="$parent.Icon" relativePoint="RIGHT" x="-2" />
					</Anchors>
				</FontString>
			</Layer>
		</Layers>
	</Frame>
	<Frame name="UIWidgetTemplateStackedResourceTracker" inherits="UIWidgetBaseTemplate" virtual="true" secureMixin="UIWidgetTemplateStackedResourceTrackerMixin">
		<Layers>
			<Layer level="BORDER">
				<Texture parentKey="Frame" useAtlasSize="true">
					<Anchors>
						<Anchor point="TOPRIGHT" x="-45" />
					</Anchors>
				</Texture>
			</Layer>
		</Layers>
	</Frame>
</Ui>
